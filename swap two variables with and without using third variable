import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	int b=sc.nextInt();
	int temp=0;
	System.out.print("with third variable");
	temp=a;
	a=b;
	b=temp;
	System.out.println(a+" "+b);
	System.out.print("without using third variable");
    a=a+b;
    b=a-b;
    a=a-b;
    System.out.println(a+" "+b);
}
}

