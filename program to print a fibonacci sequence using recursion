import java.util.*;
public class Main
{
	public static int fabonacci(int n){
	    if(n==0){
	        return 0;
	    }
	    else if(n==1){
	        return 1;
	    }
	    else{
	        return  fabonacci(n-1)+ fabonacci(n-2);
	    }
	}
		public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	System.out.println(fabonacci(n));
	
}
}
