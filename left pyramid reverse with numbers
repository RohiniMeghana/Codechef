import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int r=sc.nextInt();
       for(int i=r;i>0;i--){
           for(int j=0;j<i;j++){
               System.out.print(j+1+" ");
           }
          
          System.out.println("");
           }
           
       }
    }
