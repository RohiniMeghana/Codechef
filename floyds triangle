import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int r=sc.nextInt();
       int n=1;
       for(int i=0;i<r;i++){
           for(int j=0;j<=i;j++){
               System.out.print(n+" ");
               n++;
           }
          
          System.out.println("");
           }
           
       }
    }
