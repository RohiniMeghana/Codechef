import java.util.*;
class HelloWorld {
    public static long fib(int n){
        if(n==0){
            return 0L;
        }
        else if(n==1){
            return 1L;
        }
        else{
            return fib(n-1)+fib(n-2);
        }
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       long a=fib(n);
        System.out.println(a);
}
}
